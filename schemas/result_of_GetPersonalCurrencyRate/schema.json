{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "description": "Ответ на запрос цены валюты для купли и продажи для указанного пользователя",
    "properties": {
        "price_buy": {
            "type": "number",
            "exclusiveMinimum": 0,
            "description": "Цена покупки валюты"
        },
        "price_sell": {
            "type": "number",
            "exclusiveMinimum": 0,
            "description": "Цена продажи валюты"
        }
    },
    "required": [
        "price_buy",
        "price_sell"
    ],
    "examples": [
        {
            "price_sell": 30.2,
            "price_buy": 40.5
        }
    ]
}